<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>rpg_character_creator</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="images\elf.png" />
    <None Remove="images\empty.png" />
    <None Remove="images\furry.png" />
    <None Remove="images\human.png" />
    <None Remove="images\reptilian.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="images\elf.png" />
    <Resource Include="images\empty.png" />
    <Resource Include="images\furry.png" />
    <Resource Include="images\human.png" />
    <Resource Include="images\reptilian.png" />
  </ItemGroup>

</Project>
